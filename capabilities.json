                        "max":1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "for": {
                        "in": "Category"
                    },
                    "dataReductionAlgorithm": {
                        "top": {
                            "count": 30000
                        }
                    }
                },
                "values": {
